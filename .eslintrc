{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            jsx: true,
            experimentalObjectRestSpread: true
        },
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "global-require": 0,
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ],
        "no-underscore-dangle": 0,
        "max-len": 0,
        "new-cap": 0,
        "no-param-reassign": 0,
        "one-var": 0,
        "comma-dangle": 0,
        "no-cond-assign": 0,
        "no-nested-ternary": 0,
        "spaced-comment": 0,
        "guard-for-in": 0,
        "camelcase": 0,
        "func-names": 0,
        "no-use-before-define": 0,
        "no-constant-condition": 0,
        "no-shadow": 0,
        "padded-blocks": 0,
        "no-console": 0,
        "space-return-throw-case": 0,
        "space-before-keywords": 0,
        "space-after-keywords": 0,
        "no-empty-label": 0,
        "no-restricted-syntax": 0,
        no-labels: ["error"],
        keyword-spacing: ["error", { "before": true, "after": true }]
    }
}
